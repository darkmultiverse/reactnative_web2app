{
  "expo": {
    "name": "SiteApp",
    "slug": "SiteApp",
    "version": "1.1.201",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#423a3a"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.app.site",
      "buildNumber": "1.1.201"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#423a3a"
      },
      "package": "com.fans.onlyfans"
    },
    "androidStatusBar": {
      "backgroundColor": "#073af2",
      "translucent": true
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
